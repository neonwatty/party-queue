{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "project": [
    "app/**/*.{ts,tsx}",
    "lib/**/*.{ts,tsx}",
    "components/**/*.{ts,tsx}",
    "hooks/**/*.{ts,tsx}",
    "contexts/**/*.{ts,tsx}",
    "types/**/*.{ts,tsx}",
    "utils/**/*.{ts,tsx}"
  ],
  "ignore": [
    "lib/notifications.ts",
    "lib/analytics.ts",
    "lib/retry.ts",
    "lib/supabase/client.ts",
    "lib/supabase/server.ts",
    "hooks/useAnalytics.ts",
    "hooks/useError.ts",
    "hooks/useErrorHandler.ts",
    "hooks/useOnlineStatus.ts",
    "hooks/usePWAInstall.ts",
    "components/ui/index.ts",
    "components/ui/InstallPrompt.tsx",
    "components/ui/OfflineIndicator.tsx",
    "types/index.ts"
  ],
  "ignoreDependencies": [
    "@capacitor/core",
    "@capacitor/ios",
    "@capacitor/push-notifications",
    "@testing-library/user-event",
    "@vitest/coverage-v8",
    "tailwindcss",
    "postcss",
    "@supabase/ssr"
  ],
  "ignoreBinaries": ["doppler"],
  "ignoreExportsUsedInFile": true,
  "rules": {
    "exports": "off"
  },
  "next": {
    "entry": ["app/**/*.{ts,tsx}", "next.config.ts"]
  },
  "vitest": {
    "entry": ["**/*.{test,spec}.{ts,tsx}", "vitest.config.ts"]
  }
}
